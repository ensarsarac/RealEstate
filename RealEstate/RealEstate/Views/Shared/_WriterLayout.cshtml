
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("/Views/Writer/HeadPartial.cshtml") 
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        @await Html.PartialAsync("/Views/Writer/NavbarPartial.cshtml") 
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            @await Html.PartialAsync("/Views/Writer/SideBarPartial.cshtml")

            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                @await Html.PartialAsync("/Views/Writer/FooterPartial.cshtml")

                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
@await Html.PartialAsync("/Views/Writer/ScriptPartial.cshtml") 
    @RenderSection("Script",false)
</body>
</html>
